<?xml version='1.0' encoding='UTF-8'?>
<ui:composition xmlns:ui="http://java.sun.com/jsf/facelets" xmlns:af="http://xmlns.oracle.com/adf/faces/rich"
                xmlns:f="http://java.sun.com/jsf/core" xmlns:h="http://java.sun.com/jsf/html">
    <af:panelGroupLayout id="pgl3" halign="center" layout="horizontal">
        <af:train value="#{controllerContext.currentViewPort.taskFlowContext.trainModel}"
         visible="false" id="t1"/>
    </af:panelGroupLayout>
    <af:pageTemplate viewId="/oracle/apps/uikit/common/pageTemplates/MainPageTemplate.jspx" id="pt1">
        <f:facet name="pageContents">
            <af:panelBox text="Create Dependent" id="pb1" showDisclosure="false" styleClass="shadowBorder"
                         inlineStyle="border-color: transparent ;" background="medium">
                <f:facet name="toolbar">
                    <af:toolbar id="t2">
                        <af:button text="Back" id="b1" styleClass="blue-proceed"
                                   action="#{controllerContext.currentViewPort.taskFlowContext.trainModel.getPrevious}"/>
                        <af:button text="Next" id="b3" styleClass="blue-proceed"
                                   visible="#{bindings.XxhrHmoTrxDtl_EOView2.estimatedRowCount ne 0}"
                                   action="#{backingBeanScope.AddEditHmoBB.depNextAction}"/>
                        <af:button text="Cancel" id="b5" styleClass="red-cancel"
                                   action="#{backingBeanScope.AddEditHmoBB.cancelActionDep}"/>
                    </af:toolbar>
                </f:facet>
                <af:panelGroupLayout id="pgl10" layout="vertical">
                    <af:spacer width="10" height="10" id="s20"/>
                    <af:panelGridLayout id="pgl7" inlineStyle="background:#e0e0e0;">
                        <af:gridRow marginTop="5px" height="auto" id="gr6">
                            <af:gridCell marginStart="5px" width="25%" id="gc3" halign="center">
                                <af:outputText value="Request No" id="ot6"
                                               inlineStyle="font-size:larger; font-weight: bold;"/>
                            </af:gridCell>
                            <af:gridCell marginStart="5px" width="25%" marginEnd="5px" id="gc4" halign="center">
                                <af:outputText value="Request Date" id="ot7"
                                               inlineStyle="font-size:larger; font-weight: bold;"/>
                            </af:gridCell>
                            <af:gridCell marginStart="5px" width="25%" marginEnd="5px" id="gc22" halign="center">
                                <af:outputText value="Request Status" id="ot10"
                                               inlineStyle="font-size:larger; font-weight: bold;"/>
                            </af:gridCell>
                            <af:gridCell marginStart="5px" width="25%" marginEnd="5px" id="gc17" halign="center">
                                <af:outputText value="Maximum Limit" id="ot3"
                                               inlineStyle="font-size:larger; font-weight: bold;"/>
                            </af:gridCell>
                        </af:gridRow>
                        <af:gridRow marginTop="5px" height="auto" marginBottom="5px" id="gr7">
                            <af:gridCell marginStart="5px" width="25%" id="gc5" halign="center">
                                <af:outputText value="#{bindings.HmoRequestNo.inputValue}" id="ot8"
                                               inlineStyle="font-size:larger;"/>
                            </af:gridCell>
                            <af:gridCell marginStart="5px" width="25%" marginEnd="5px" id="gc7" halign="center">
                                <af:outputText value="#{bindings.HmoRequestDate.inputValue}" id="ot9"
                                               inlineStyle="font-size:larger;">
                                    <af:convertDateTime pattern="dd-MMM-yyyy"/>
                                </af:outputText>
                            </af:gridCell>
                            <af:gridCell marginStart="5px" width="25%" marginEnd="5px" id="gc23" halign="center">
                                <af:outputText value="#{bindings.ApprovalStatus.inputValue}" id="ot11"
                                               inlineStyle="font-size:larger;"/>
                            </af:gridCell>
                            <af:gridCell marginStart="5px" width="25%" marginEnd="5px" id="gc18" halign="center">
                                <af:outputText value="#{bindings.Trans_EligibleCost.inputValue}" id="ot4"
                                               inlineStyle="font-size:larger;">
                                    <af:convertNumber minFractionDigits="#{sessionScope.MinFractionDigits}"
                                                      maxFractionDigits="#{sessionScope.MaxFractionDigits}"
                                                      pattern="#{sessionScope.NumberFormat}"
                                                      minIntegerDigits="#{sessionScope.MinIntegerDigits}"/>
                                </af:outputText>
                            </af:gridCell>
                        </af:gridRow>
                    </af:panelGridLayout>
                    <af:spacer width="10" height="10" id="s1"/>
                    <af:panelBox text="Add Dependent Details" id="pb2" showDisclosure="false"
                                 inlineStyle="height:12cm;background-color: White;">
                        <af:table value="#{bindings.XxhrHmoTrxDtl_EOView2.collectionModel}" var="row"
                                  rows="#{bindings.XxhrHmoTrxDtl_EOView2.rangeSize}"
                                  emptyText="#{bindings.XxhrHmoTrxDtl_EOView2.viewable ? 'No data to display.' : 'Access Denied.'}"
                                  rowBandingInterval="0"
                                  selectedRowKeys="#{bindings.XxhrHmoTrxDtl_EOView2.collectionModel.selectedRow}"
                                  selectionListener="#{bindings.XxhrHmoTrxDtl_EOView2.collectionModel.makeCurrent}"
                                  rowSelection="single" fetchSize="#{bindings.XxhrHmoTrxDtl_EOView2.rangeSize}" id="t6"
                                  columnStretching="column:c3">
                            <af:column id="c22" headerText="Photo" align="center">
                                <af:image source="/profilepictureservlet?type=line&amp;id=#{row.HmoTrxDtlId}"
                                          rendered="#{row.DepPhoto ne null}" shortDesc="" id="i5"
                                          inlineStyle="height:2.3cm; width:2.3cm;"/>
                                <af:image source="/images/no_image.png" id="i2"
                                          rendered="#{row.DepPhoto eq null || row.DepPhoto eq ''}"
                                          inlineStyle="height:2.3cm; width:2.3cm;"/>
                                <af:link text="Update Photo" id="l5">
                                    <af:showPopupBehavior popupId="::p2"/>
                                </af:link>
                            </af:column>
                            <af:column id="c23" headerText="Details" align="center" width="400" noWrap="false">
                                <af:panelGridLayout id="pgl4">
                                    <af:gridRow marginTop="5px" height="auto" id="gr1">
                                        <af:gridCell marginStart="5px" width="50%" id="gc1">
                                            <af:outputText value="#{row.DepTitle} #{row.DepFirstName} #{row.DepLastName}"
                                                           inlineStyle="font-size:larger; font-weight: bold;"
                                                           id="ot21"/>
                                        </af:gridCell>
                                        <af:gridCell marginStart="5px" width="50%" id="gc2">
                                            <af:panelGroupLayout id="pgl14" layout="horizontal">
                                                <af:outputText value="DOB :" id="aot4"
                                                               inlineStyle="font-weight:bolder;"/>
                                                <af:spacer width="10" height="10" id="s7"/>
                                                <af:outputFormatted value="#{row.DepDob}" id="of6">
                                                    <af:convertDateTime pattern="dd-MMM-yyyy"/>
                                                </af:outputFormatted>
                                            </af:panelGroupLayout>
                                        </af:gridCell>
                                    </af:gridRow>
                                    <af:gridRow marginTop="5px" height="auto" id="gr2">
                                        <af:gridCell marginStart="5px" width="50%" id="gc9">
                                            <af:outputText value="#{row.DependentType}" id="ot2"/>
                                        </af:gridCell>
                                        <af:gridCell marginStart="5px" width="50%" id="gc10">
                                            <af:panelGroupLayout id="pgl13" layout="horizontal">
                                                <af:image source="/images/func_phone_16_ena.png"
                                                          inlineStyle="width:10px; height:10px;" shortDesc="Mobile"
                                                          id="i3"/>
                                                <af:spacer width="10" height="10" id="s6"/>
                                                <af:outputFormatted value="#{row.MobileNo}" id="of9"></af:outputFormatted>
                                            </af:panelGroupLayout>
                                        </af:gridCell>
                                    </af:gridRow>
                                    <af:gridRow marginTop="5px" height="auto" marginBottom="5px" id="gr3">
                                        <af:gridCell marginStart="5px" width="50%" id="gc13">
                                            <af:outputText value="#{row.DepGender}" id="ot20"/>
                                        </af:gridCell>
                                        <af:gridCell marginStart="5px" width="50%" id="gc14">
                                            <af:outputText value="#{row.DepAgeCategory}" id="ot22"
                                                           shortDesc="#{row.DepAgeCategory}"/>
                                        </af:gridCell>
                                    </af:gridRow>
                                </af:panelGridLayout>
                            </af:column>
                            <af:column id="c3" width="200" noWrap="false">
                                <af:panelGridLayout id="pgl5">
                                    <af:gridRow marginTop="5px" height="auto" id="gr9">
                                        <af:gridCell marginStart="5px" width="100%" id="gc6">
                                            <af:outputFormatted value="#{row.ResidentialAddress}" id="of5"/>
                                        </af:gridCell>
                                    </af:gridRow>
                                    <af:gridRow marginTop="5px" height="auto" id="gr10">
                                        <af:gridCell marginStart="5px" width="100%" id="gc11">
                                            <af:outputText value="State Of Residence :" id="ot12"
                                                           inlineStyle="font-weight:bolder;"/>
                                            <af:spacer width="10" height="10" id="s10"/>
                                            <af:outputFormatted value="#{row.StateOfResidence}" id="of7"/>
                                        </af:gridCell>
                                    </af:gridRow>
                                    <af:gridRow marginTop="5px" height="auto" marginBottom="5px" id="gr11">
                                        <af:gridCell marginStart="5px" width="100%" id="gc15">
                                            <af:outputText value="Hospital :" id="ot13"
                                                           inlineStyle="font-weight:bolder;"/>
                                            <af:spacer width="10" height="10" id="s11"/>
                                            <af:outputText value="#{row.Hospital}" id="of2"
                                                           shortDesc="#{row.Hospital}"/>
                                        </af:gridCell>
                                    </af:gridRow>
                                </af:panelGridLayout>
                            </af:column>
                            <af:column id="c2" headerText="Cost Details" width="250"
                            rendered="#{bindings.HmoCategory.inputValue eq 'Traditional Dependent Categorization' ? true : true}">
                                <af:panelGridLayout id="pgl2">
                                    <af:gridRow marginTop="5px" height="auto" id="gr4">
                                        <af:gridCell marginStart="5px" width="100%" marginEnd="5px" id="gc8"
                                                     rendered="#{bindings.HmoCategory.inputValue ne 'Traditional Dependent Categorization'}">
                                            <af:outputText value="Dependent Cost :" id="ot1"
                                                           inlineStyle="font-weight:bolder;"/>
                                            <af:spacer width="10" height="10" id="s8"/>
                                            <af:outputFormatted value="#{row.DependentCost}" id="of8">
                                                <af:convertNumber minFractionDigits="#{sessionScope.MinFractionDigits}"
                                                                  maxFractionDigits="#{sessionScope.MaxFractionDigits}"
                                                                  pattern="#{sessionScope.NumberFormat}"
                                                                  minIntegerDigits="#{sessionScope.MinIntegerDigits}"/>
                                            </af:outputFormatted>
                                        </af:gridCell>
                                    </af:gridRow>
                                    <af:gridRow marginTop="5px" height="auto" id="gr5"  rendered="#{bindings.HmoCategory.inputValue ne 'Traditional Dependent Categorization'}" >
                                        <af:gridCell marginStart="5px" width="100%" marginEnd="5px" id="gc12">
                                            <af:outputText value="Total Cost :" id="ot5"
                                                           inlineStyle="font-weight:bolder;"/>
                                            <af:spacer width="10" height="10" id="s2"/>
                                            <af:outputFormatted value="#{bindings.HmoPlanTotal.inputValue}" id="of1">
                                                <af:convertNumber minFractionDigits="#{sessionScope.MinFractionDigits}"
                                                                  maxFractionDigits="#{sessionScope.MaxFractionDigits}"
                                                                  pattern="#{sessionScope.NumberFormat}"
                                                                  minIntegerDigits="#{sessionScope.MinIntegerDigits}"/>
                                            </af:outputFormatted>
                                        </af:gridCell>
                                    </af:gridRow>
                                    <af:gridRow marginTop="5px" height="auto" marginBottom="5px" id="gr8">
                                        <af:gridCell marginStart="5px" width="100%" marginEnd="5px" id="gc16"
                                                    >
                                            <af:outputText value="Amount Payable :" id="ot14"
                                                           inlineStyle="font-weight:bolder;"/>
                                            <af:spacer width="10" height="10" id="s19"/>
                                            <af:outputFormatted value="#{bindings.HmoPlanDeduction.inputValue}"
                                                                id="of3">
                                                <af:convertNumber minFractionDigits="#{sessionScope.MinFractionDigits}"
                                                                  maxFractionDigits="#{sessionScope.MaxFractionDigits}"
                                                                  pattern="#{sessionScope.NumberFormat}"
                                                                  minIntegerDigits="#{sessionScope.MinIntegerDigits}"/>
                                            </af:outputFormatted>
                                        </af:gridCell>
                                    </af:gridRow>
                                </af:panelGridLayout>
                            </af:column>
                            <af:column id="c8" headerText="Action" width="150" noWrap="false">
                                <af:panelGroupLayout id="pgl12" layout="vertical">
                                    <af:link text="Edit" icon="/images/edit_ena.png" hoverIcon="/images/edit_ovr.png"
                                             depressedIcon="/images/edit_ovr.png" disabledIcon="/images/edit_dis.png"
                                             id="l3"
                                             actionListener="#{backingBeanScope.AddEditHmoBB.onClickEditDependent}">
                                        <af:setPropertyListener from="EDIT" to="#{pageFlowScope.ADD_EDIT_MODE}"
                                                                type="action"/>
                                    </af:link>
                                    <af:spacer width="10" height="10" id="s4"/>
                                    <af:link id="l2" icon="/images/attach_qualifier.png"
                                             text="Evidence of DOB [ #{row.EvidenceCount} ]">
                                        <af:showPopupBehavior popupId="::p3"/>
                                    </af:link>
                                    <af:spacer width="10" height="10" id="s5"/>
                                    <af:link actionListener="#{bindings.Delete.execute}" text="Delete"
                                             icon="/images/func_remove_16_ena.png"
                                              disabled="#{bindings.HmoAcceptanceFlag.inputValue eq 'Y'
                                             || bindings.ApprovalStatus.inputValue eq 'SUBMITTED FOR APPROVAL' 
                                             || bindings.ApprovalStatus.inputValue eq 'APPROVED'
                                             ||  bindings.ApprovalStatus.inputValue eq 'REJECTED'}"  
                                             hoverIcon="/images/func_remove_16_hov.png"
                                             depressedIcon="/images/func_report_16_act.png"
                                             disabledIcon="/images/func_remove_16_dis.png" id="l4"/>
                                </af:panelGroupLayout>
                            </af:column>
                        </af:table>
                        <f:facet name="toolbar">
                            <af:toolbar id="t4">
                                <af:link icon="/images/qual_added_16.png" hoverIcon="/images/func_add_16_hov.png"
                                         depressedIcon="/images/func_add_16_act.png" disabled="#{((bindings.HmoCategory.inputValue eq 'Traditional Dependent Categorization')
and (bindings.XxhrHmoTrxDtl_EOView2.estimatedRowCount == 4) and (bindings.HmoOption.inputValue eq 'Employee + Dependant Children')) 
|| ((bindings.XxhrHmoTrxDtl_EOView2.estimatedRowCount == 1) and (bindings.HmoOption.inputValue eq 'Employee +1 Dependant Child')) || 
bindings.ApprovalStatus.inputValue eq 'SUBMITTED FOR APPROVAL' || bindings.ApprovalStatus.inputValue eq 'APPROVED'
||  bindings.ApprovalStatus.inputValue eq 'REJECTED'? true : false}" disabledIcon="/images/func_add_16_dis.png"
                                         text="Add Dependent" id="l1"
                                         actionListener="#{backingBeanScope.AddEditHmoBB.onClickAddDependent}">
                                    <af:setPropertyListener from="ADD" to="#{pageFlowScope.ADD_EDIT_MODE}"
                                                            type="action"/>
                                </af:link>
                            </af:toolbar>
                        </f:facet>
                    </af:panelBox>
                    <af:popup childCreation="deferred" autoCancel="disabled" id="p1"
                              binding="#{backingBeanScope.AddEditHmoBB.dependentPop}">
                        <af:dialog id="d1" type="none"
                                   title="#{pageFlowScope.ADD_EDIT_MODE eq 'ADD' ? 'Add':'Edit'} Dependent"
                                   closeIconVisible="false" titleIconSource="/images/audiencesegmenttree_24_ena.png">
                            <f:facet name="buttonBar">
                                <af:panelGroupLayout id="pgl1">
                                    <af:button text="Ok" id="b2" styleClass="blue-proceed"
                                             disabled="#{bindings.HmoAcceptanceFlag.inputValue eq 'Y'
                                             || bindings.ApprovalStatus.inputValue eq 'SUBMITTED FOR APPROVAL' 
                                             || bindings.ApprovalStatus.inputValue eq 'APPROVED'
                                             ||  bindings.ApprovalStatus.inputValue eq 'REJECTED'}"  
                                             actionListener="#{backingBeanScope.AddEditHmoBB.onClickOkDepPop}">
                                        <af:resetListener type="action"/>
                                    </af:button>
                                    <af:button text="Cancel" id="b4" immediate="true" styleClass="blue-proceed"
                                               actionListener="#{backingBeanScope.AddEditHmoBB.onCancelDepPop}">
                                        <af:resetListener type="action"/>
                                    </af:button>
                                </af:panelGroupLayout>
                            </f:facet>
                            <af:panelFormLayout id="pfl2" labelWidth="150" labelAlignment="start" maxColumns="3"
                                                rows="8">
                                <af:selectOneChoice value="#{bindings.DependentType1.inputValue}" label="Dependent Type"
                                                    autoSubmit="true" required="true"
                                                    disabled="#{bindings.HmoAcceptanceFlag.inputValue eq 'Y'
                                             || bindings.ApprovalStatus.inputValue eq 'SUBMITTED FOR APPROVAL' 
                                             || bindings.ApprovalStatus.inputValue eq 'APPROVED'
                                             ||  bindings.ApprovalStatus.inputValue eq 'REJECTED'}"
                                                    shortDesc="#{bindings.DependentType1.hints.tooltip}" id="soc1"
                                                    valueChangeListener="#{backingBeanScope.AddEditHmoBB.onChangeDepType}">
                                    <f:selectItems value="#{bindings.DependentType1.items}" id="si1"/>
                                    <f:validator binding="#{bindings.DependentType1.validator}"/>
                                </af:selectOneChoice>
                                <af:selectOneChoice value="#{bindings.DepTitle.inputValue}"
                                disabled="#{bindings.HmoAcceptanceFlag.inputValue eq 'Y'
                                             || bindings.ApprovalStatus.inputValue eq 'SUBMITTED FOR APPROVAL' 
                                             || bindings.ApprovalStatus.inputValue eq 'APPROVED'
                                             ||  bindings.ApprovalStatus.inputValue eq 'REJECTED'}"
                                                    label="#{bindings.DepTitle.label}" autoSubmit="true" required="true"
                                                    shortDesc="#{bindings.DepTitle.hints.tooltip}" id="soc2">
                                    <f:selectItems value="#{bindings.DepTitle.items}" id="si2"/>
                                    <f:validator binding="#{bindings.DepTitle.validator}"/>
                                </af:selectOneChoice>
                                <af:inputText value="#{bindings.DepFirstName.inputValue}"
                                              label="#{bindings.DepFirstName.hints.label}" required="true"
                                              contentStyle="width:300px;" autoSubmit="true"
                                              disabled="#{bindings.HmoAcceptanceFlag.inputValue eq 'Y'
                                             || bindings.ApprovalStatus.inputValue eq 'SUBMITTED FOR APPROVAL' 
                                             || bindings.ApprovalStatus.inputValue eq 'APPROVED'
                                             ||  bindings.ApprovalStatus.inputValue eq 'REJECTED'}"
                                              columns="#{bindings.DepFirstName.hints.displayWidth}"
                                              maximumLength="#{bindings.DepFirstName.hints.precision}"
                                              shortDesc="#{bindings.DepFirstName.hints.tooltip}" id="it4">
                                    <f:validator binding="#{bindings.DepFirstName.validator}"/>
                                </af:inputText>
                                <af:inputText value="#{bindings.DepLastName.inputValue}"
                                              label="#{bindings.DepLastName.hints.label}" contentStyle="width:300px;"
                                              autoSubmit="true" required="true"
                                              disabled="#{bindings.HmoAcceptanceFlag.inputValue eq 'Y'
                                             || bindings.ApprovalStatus.inputValue eq 'SUBMITTED FOR APPROVAL' 
                                             || bindings.ApprovalStatus.inputValue eq 'APPROVED'
                                             ||  bindings.ApprovalStatus.inputValue eq 'REJECTED'}"
                                              columns="#{bindings.DepLastName.hints.displayWidth}"
                                              maximumLength="#{bindings.DepLastName.hints.precision}"
                                              shortDesc="#{bindings.DepLastName.hints.tooltip}" id="it5">
                                    <f:validator binding="#{bindings.DepLastName.validator}"/>
                                </af:inputText>
                                <af:selectOneRadio value="#{bindings.DepGender.inputValue}" autoSubmit="true"
                                                   label="#{bindings.DepGender.label}" layout="horizontal"
                                                   disabled="#{bindings.HmoAcceptanceFlag.inputValue eq 'Y'
                                             || bindings.ApprovalStatus.inputValue eq 'SUBMITTED FOR APPROVAL' 
                                             || bindings.ApprovalStatus.inputValue eq 'APPROVED'
                                             ||  bindings.ApprovalStatus.inputValue eq 'REJECTED'}"
                                                   required="true" shortDesc="#{bindings.DepGender.hints.tooltip}"
                                                   id="sor1">
                                    <f:selectItems value="#{bindings.DepGender.items}" id="si4"/>
                                    <f:validator binding="#{bindings.DepGender.validator}"/>
                                </af:selectOneRadio>
                      
                                <af:inputDate value="#{bindings.DepDob.inputValue}" contentStyle="width:300px;"
                                              autoSubmit="true" label="#{bindings.DepDob.hints.label}" required="true"
                                              columns="#{bindings.DepDob.hints.displayWidth}"
                                              disabled="#{bindings.HmoAcceptanceFlag.inputValue eq 'Y'
                                             || bindings.ApprovalStatus.inputValue eq 'SUBMITTED FOR APPROVAL' 
                                             || bindings.ApprovalStatus.inputValue eq 'APPROVED'
                                             ||  bindings.ApprovalStatus.inputValue eq 'REJECTED'}"
                                              shortDesc="#{bindings.DepDob.hints.tooltip}" id="id1"
                                              valueChangeListener="#{backingBeanScope.AddEditHmoBB.onChangeDepDOB}"
                                              maxValue="#{backingBeanScope.AddEditHmoBB.currentSystemDate}"
                                              binding="#{backingBeanScope.AddEditHmoBB.dependDob}">
                                    <f:validator binding="#{bindings.DepDob.validator}"/>
                                    <af:convertDateTime pattern="dd-MMM-yyyy"/>
                                </af:inputDate>                                
                                <af:inputText value="#{bindings.DepAgeCategory.inputValue}"
                                              label="#{bindings.DepAgeCategory.hints.label}"
                                              required="#{bindings.DepAgeCategory.hints.mandatory}"
                                              contentStyle="width:300px;" autoSubmit="true" partialTriggers="id1"
                                              disabled="true" columns="#{bindings.DepAgeCategory.hints.displayWidth}"
                                              maximumLength="#{bindings.DepAgeCategory.hints.precision}"
                                              shortDesc="#{bindings.DepAgeCategory.hints.tooltip}" id="it3">
                                    <f:validator binding="#{bindings.DepAgeCategory.validator}"/>
                                </af:inputText>
                                 <af:outputText value="Age Next Birthday"  inlineStyle="color:red;"
                                visible="#{bindings.DepDob.inputValue eq null ? false :true}"
                                       id="ot38" partialTriggers="id1"/>
                                <af:inputText value="#{bindings.MobileNo.inputValue}"
                                              label="#{bindings.MobileNo.hints.label}" contentStyle="width:200px;"
                                              autoSubmit="true" required="false"
                                              disabled="#{bindings.HmoAcceptanceFlag.inputValue eq 'Y'
                                             || bindings.ApprovalStatus.inputValue eq 'SUBMITTED FOR APPROVAL' 
                                             || bindings.ApprovalStatus.inputValue eq 'APPROVED'
                                             ||  bindings.ApprovalStatus.inputValue eq 'REJECTED'}"
                                              columns="#{bindings.MobileNo.hints.displayWidth}"
                                              maximumLength="#{bindings.MobileNo.hints.precision}"
                                              shortDesc="#{bindings.MobileNo.hints.tooltip}" id="it7">
                                    <f:validator binding="#{bindings.MobileNo.validator}"/>
                                    <af:convertNumber type="number" integerOnly="true" pattern="##########"/>
                                </af:inputText>
                                <af:inputText value="#{bindings.DependentCost.inputValue}" label="Dependent Cost"
                                              contentStyle="width:200px;" partialTriggers="id1" autoSubmit="true"
                                              required="#{bindings.DependentCost.hints.mandatory}"
                                              rendered="#{bindings.HmoCategory.inputValue ne 'Traditional Dependent Categorization'}"
                                              columns="#{bindings.DependentCost.hints.displayWidth}" disabled="true"
                                              maximumLength="#{bindings.DependentCost.hints.precision}"
                                              shortDesc="#{bindings.DependentCost.hints.tooltip}" id="it9">
                                    <f:validator binding="#{bindings.DependentCost.validator}"/>
                                    <af:convertNumber minFractionDigits="#{sessionScope.MinFractionDigits}"
                                                      maxFractionDigits="#{sessionScope.MaxFractionDigits}"
                                                      pattern="#{sessionScope.NumberFormat}"
                                                      minIntegerDigits="#{sessionScope.MinIntegerDigits}"/>
                                </af:inputText>
                                <af:inputText value="#{bindings.HmoPlanTotal.inputValue}" partialTriggers="id1"
                                              label="Total Cost" disabled="true" contentStyle="width:200px;"
                                              required="#{bindings.HmoPlanTotal.hints.mandatory}" rendered="true"
                                              columns="#{bindings.HmoPlanTotal.hints.displayWidth}"
                                              maximumLength="#{bindings.HmoPlanTotal.hints.precision}"
                                              shortDesc="#{bindings.HmoPlanTotal.hints.tooltip}" id="it2">
                                    <f:validator binding="#{bindings.HmoPlanTotal.validator}"/>
                                    <af:convertNumber minFractionDigits="#{sessionScope.MinFractionDigits}"
                                                      maxFractionDigits="#{sessionScope.MaxFractionDigits}"
                                                      pattern="#{sessionScope.NumberFormat}"
                                                      minIntegerDigits="#{sessionScope.MinIntegerDigits}"/>
                                </af:inputText>
                                <af:inputText value="#{bindings.Trans_EligibleCost.inputValue}" label="Maximum Limit"
                                              disabled="true" contentStyle="width:200px;" partialTriggers="id1"
                                              required="#{bindings.Trans_EligibleCost.hints.mandatory}"
                                           
                                              columns="#{bindings.Trans_EligibleCost.hints.displayWidth}"
                                              maximumLength="#{bindings.Trans_EligibleCost.hints.precision}"
                                              shortDesc="#{bindings.Trans_EligibleCost.hints.tooltip}" id="it6">
                                    <f:validator binding="#{bindings.Trans_EligibleCost.validator}"/>
                                    <af:convertNumber minFractionDigits="#{sessionScope.MinFractionDigits}"
                                                      maxFractionDigits="#{sessionScope.MaxFractionDigits}"
                                                      pattern="#{sessionScope.NumberFormat}"
                                                      minIntegerDigits="#{sessionScope.MinIntegerDigits}"/>
                                </af:inputText>
                                <af:inputText value="#{bindings.HmoPlanDeduction.inputValue}" partialTriggers="id1"
                                              label="Amount Payable" disabled="true" contentStyle="width:200px;"
                                              required="#{bindings.HmoPlanDeduction.hints.mandatory}"
                                              columns="#{bindings.HmoPlanDeduction.hints.displayWidth}"
                                              maximumLength="#{bindings.HmoPlanDeduction.hints.precision}"
                                              shortDesc="#{bindings.HmoPlanDeduction.hints.tooltip}" id="it1">
                                    <f:validator binding="#{bindings.HmoPlanDeduction.validator}"/>
                                    <af:convertNumber minFractionDigits="#{sessionScope.MinFractionDigits}"
                                                      maxFractionDigits="#{sessionScope.MaxFractionDigits}"
                                                      pattern="#{sessionScope.NumberFormat}"
                                                      minIntegerDigits="#{sessionScope.MinIntegerDigits}"/>
                                </af:inputText>
                                <af:inputText value="#{bindings.ResidentialAddress.inputValue}"
                                              label="Residential Address" contentStyle="width:200px;" autoSubmit="true"
                                              required="true"
                                              disabled="#{bindings.HmoAcceptanceFlag.inputValue eq 'Y'
                                             || bindings.ApprovalStatus.inputValue eq 'SUBMITTED FOR APPROVAL' 
                                             || bindings.ApprovalStatus.inputValue eq 'APPROVED'
                                             ||  bindings.ApprovalStatus.inputValue eq 'REJECTED'}"
                                              columns="#{bindings.ResidentialAddress.hints.displayWidth}"
                                              maximumLength="#{bindings.ResidentialAddress.hints.precision}"
                                              shortDesc="#{bindings.ResidentialAddress.hints.tooltip}" id="it8"
                                              rows="3">
                                    <f:validator binding="#{bindings.ResidentialAddress.validator}"/>
                                </af:inputText>
                            </af:panelFormLayout>
                            <af:spacer width="10" height="10" id="s3"/>
                            <af:panelFormLayout id="pfl1" maxColumns="1" rows="2">
                                <af:inputListOfValues id="stateOfResidenceId"
                                                      popupTitle="Search and Select: #{bindings.StateOfResidence.hints.label}"
                                                      value="#{bindings.StateOfResidence.inputValue}"
                                                      contentStyle="width:600px;" autoSubmit="true"
                                                      label="#{bindings.StateOfResidence.hints.label}"
                                                      disabled="#{bindings.HmoAcceptanceFlag.inputValue eq 'Y'
                                             || bindings.ApprovalStatus.inputValue eq 'SUBMITTED FOR APPROVAL' 
                                             || bindings.ApprovalStatus.inputValue eq 'APPROVED'
                                             ||  bindings.ApprovalStatus.inputValue eq 'REJECTED'}"
                                                      model="#{bindings.StateOfResidence.listOfValuesModel}"
                                                      
                                                      required="true" rendered="#{bindings.HmoType.inputValue eq 'BUPA' || bindings.HmoType.inputValue eq 'CIGNA' ||
                                                        bindings.HmoType.inputValue eq 'CIGNA NIGERIA - INTERNATIONAL' ||
                                                        bindings.HmoType.inputValue eq 'CIGNA WORLDWIDE- INTERNATIONAL' ||  bindings.HmoType.inputValue eq 'LEADWAY HEALTH PREMIUM' ||
                                                        bindings.HmoType.inputValue eq 'CIGNA AFRICA' ? false : true}"
                                                      columns="#{bindings.StateOfResidence.hints.displayWidth}"
                                                      shortDesc="#{bindings.StateOfResidence.hints.tooltip}">
                                    <f:validator binding="#{bindings.StateOfResidence.validator}"/>
                                    <af:autoSuggestBehavior suggestItems="#{bindings.StateOfResidence.suggestItems}"
                                                            maxSuggestedItems="5"/>
                                </af:inputListOfValues>
                                <af:inputListOfValues id="hospitalId" contentStyle="width:600px;" autoSubmit="true"
                                                      popupTitle="Search and Select: #{bindings.Hospital.hints.label}"
                                                      value="#{bindings.Hospital.inputValue}"
                                                      disabled="#{bindings.HmoAcceptanceFlag.inputValue eq 'Y'
                                             || bindings.ApprovalStatus.inputValue eq 'SUBMITTED FOR APPROVAL' 
                                             || bindings.ApprovalStatus.inputValue eq 'APPROVED'
                                             ||  bindings.ApprovalStatus.inputValue eq 'REJECTED'}"
                                                      label="#{bindings.Hospital.hints.label}" rendered="#{bindings.HmoType.inputValue eq 'BUPA' || bindings.HmoType.inputValue eq 'CIGNA' ||
                                                        bindings.HmoType.inputValue eq 'CIGNA NIGERIA - INTERNATIONAL' ||
                                                        bindings.HmoType.inputValue eq 'CIGNA WORLDWIDE- INTERNATIONAL' ||
                                                        bindings.HmoType.inputValue eq 'CIGNA AFRICA' ? false : true}"
                                                      model="#{bindings.Hospital.listOfValuesModel}"
                                                      required="#{bindings.Hospital.hints.mandatory}"
                                                      columns="#{bindings.Hospital.hints.displayWidth}"
                                                      shortDesc="#{bindings.Hospital.hints.tooltip}">
                                    <f:validator binding="#{bindings.Hospital.validator}"/>
                                    <af:autoSuggestBehavior suggestItems="#{bindings.Hospital.suggestItems}"
                                                            maxSuggestedItems="5"/>
                                </af:inputListOfValues>
                            </af:panelFormLayout>
                        </af:dialog>
                    </af:popup>
                    <af:popup childCreation="deferred" autoCancel="disabled" id="p2">
                        <af:dialog id="d2" type="none" title="Update Photo">
                            <f:facet name="buttonBar"></f:facet>
                            <af:outputText value="Please Upload Image File Format contains  jpeg, jpg and png   " id="ot18"
                                           inlineStyle="color:Red;"/>
                            <af:spacer width="10" height="20" id="s12"/>
                            <af:inputFile label="Select Photo to update " id="if1" autoSubmit="true"
                            disabled="#{bindings.HmoAcceptanceFlag.inputValue eq 'Y'
                                             || bindings.ApprovalStatus.inputValue eq 'SUBMITTED FOR APPROVAL' 
                                             || bindings.ApprovalStatus.inputValue eq 'APPROVED'
                                             ||  bindings.ApprovalStatus.inputValue eq 'REJECTED'}"
                                          valueChangeListener="#{backingBeanScope.AddEditHmoBB.uploadProfileDependent}"/>
                            <af:spacer width="10" height="20" id="s9"/>
                        </af:dialog>
                    </af:popup>
                    <af:popup childCreation="deferred" autoCancel="disabled" id="p3"
                              binding="#{backingBeanScope.AddEditHmoBB.evidencePop}">
                        <af:dialog id="d3" type="none" title="Evidence of DOB"
                                   titleIconSource="/images/attach_qualifier.png" contentWidth="420">
                            <af:panelBox text="" id="pb3" showDisclosure="false">
                                <f:facet name="toolbar">
                                    <af:link actionListener="#{bindings.CreateInsertEvidence.execute}" text="Add"
                                             icon="/images/qual_added_16.png" hoverIcon="/images/func_add_16_hov.png"
                                             styleClass="blue-proceed" depressedIcon="/images/func_add_16_act.png"
                                             disabledIcon="/images/func_add_16_dis.png" id="l6"></af:link>
                                </f:facet>
                                <af:table value="#{bindings.SelfAttachment_EOView.collectionModel}" var="row"
                                          rows="#{bindings.SelfAttachment_EOView.rangeSize}"
                                          emptyText="#{bindings.SelfAttachment_EOView.viewable ? 'No data to display.' : 'Access Denied.'}"
                                          rowBandingInterval="0"
                                          selectedRowKeys="#{bindings.SelfAttachment_EOView.collectionModel.selectedRow}"
                                          selectionListener="#{bindings.SelfAttachment_EOView.collectionModel.makeCurrent}"
                                          rowSelection="single" fetchSize="#{bindings.SelfAttachment_EOView.rangeSize}"
                                          id="t3" columnStretching="column:c24" partialTriggers="l8">
                                    <af:column headerText="Attachment Name" id="c15" width="150">
                                        <af:outputText value="#{row.AttachmentName}" partialTriggers="if2"
                                                       shortDesc="#{bindings.SelfAttachment_EOView.hints.AttachmentName.tooltip}"
                                                       id="ot15"/>
                                    </af:column>
                                    <af:column headerText="Attachment Type" id="c16" width="150">
                                        <af:outputText value="#{row.AttachmentType}" partialTriggers="if2"
                                                       shortDesc="#{bindings.SelfAttachment_EOView.hints.AttachmentType.tooltip}"
                                                       id="ot16"/>
                                    </af:column>
                                    <af:column headerText="#{bindings.SelfAttachment_EOView.hints.Attachment.label}"
                                               id="c17" rendered="false">
                                        <af:outputText value="#{row.Attachment}"
                                                       shortDesc="#{bindings.SelfAttachment_EOView.hints.Attachment.tooltip}"
                                                       id="ot17"/>
                                    </af:column>
                                    <af:column id="c24" headerText="Upload" width="200">
                                        <af:inputFile id="if2" required="#{row.AttachmentName eq null ? true : false}"
                                                      valueChangeListener="#{backingBeanScope.AddEditHmoBB.onUploadFileVCL}"
                                                      autoSubmit="true"/>
                                    </af:column>
                                    <af:column id="c1" width="100" headerText="Download">
                                        <af:link icon="/images/func_download_16_act.png"
                                                 hoverIcon="/images/qual_download_16.png"
                                                 disabled="#{bindings.HmoAcceptanceFlag.inputValue eq 'Y'
                                             || bindings.ApprovalStatus.inputValue eq 'SUBMITTED FOR APPROVAL' 
                                             || bindings.ApprovalStatus.inputValue eq 'APPROVED'
                                             ||  bindings.ApprovalStatus.inputValue eq 'REJECTED'}"
                                                 disabledIcon="/images/func_download_16_ena.png" id="l7">
                                            <af:fileDownloadActionListener filename="#{row.AttachmentName}"
                                                                           method="#{backingBeanScope.AddEditHmoBB.onDownloadEvidence}"/>
                                        </af:link>
                                    </af:column>
                                    <af:column id="c29" headerText="Delete" align="center" width="50">
                                        <af:link icon="/images/func_remove_16_ena.png"
                                                 hoverIcon="/images/func_remove_16_hov.png"
                                                 depressedIcon="/images/func_report_16_act.png"
                                                 disabled="#{bindings.HmoAcceptanceFlag.inputValue eq 'Y'
                                             || bindings.ApprovalStatus.inputValue eq 'SUBMITTED FOR APPROVAL' 
                                             || bindings.ApprovalStatus.inputValue eq 'APPROVED'
                                             ||  bindings.ApprovalStatus.inputValue eq 'REJECTED'}"
                                                 disabledIcon="/images/func_remove_16_dis.png" immediate="true"
                                                 actionListener="#{bindings.DeleteAttach.execute}" id="l8"/>
                                    </af:column>
                                </af:table>
                            </af:panelBox>
                            <f:facet name="buttonBar">
                                <af:button text="Ok" id="b6" 
                                actionListener="#{backingBeanScope.AddEditHmoBB.onOkEvidPop}"/>
                            </f:facet>
                        </af:dialog>
                    </af:popup>
                </af:panelGroupLayout>
            </af:panelBox>
        </f:facet>
    </af:pageTemplate>
</ui:composition>